{
  "rewrites": [
    {
      "source": "/rozetka.xml",
      "destination": "https://online-store-project-production.up.railway.app/rozetka.xml"
    },
    {
      "source": "/gmc.xml",
      "destination": "https://online-store-project-production.up.railway.app/gmc.xml"
    },
    {
      "source": "/reset-password/:token",
      "destination": "/index.html"
    },
    {
      "source": "/product/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/blog/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}