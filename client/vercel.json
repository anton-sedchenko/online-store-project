{
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/vite"
    }
  ],
  "rewrites": [
    {
      "source": "/reset-password/:token",
      "destination": "/index.html"
    },
    {
      "source": "/product/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
